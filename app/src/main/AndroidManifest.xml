<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.android.sadeesha">

    <application
        android:name="com.demo.android.sadeesha.QuizApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.demo.android.sadeesha.view.SplashActivity"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.demo.android.sadeesha.view.ResultActivity" />
        <activity android:name="com.demo.android.sadeesha.view.QuestionActivity" />
        <activity android:name="com.demo.android.sadeesha.view.MainActivity"/>
    </application>

</manifest>